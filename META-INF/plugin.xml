<idea-plugin version="2">
    <id>org.pcsoft.plugins.intellij.inno_setup</id>
    <name>Inno Setup Script Support Plugin</name>
    <version>1.0</version>
    <vendor url="https://github.com/KleinerHacker/intellij-plugin.inno-setup">PC-Soft</vendor>

    <description><![CDATA[
      Support for Inno Setup Script Files (*.iss) and compiler (ISCC)
    ]]></description>

    <change-notes><![CDATA[

    ]]>
    </change-notes>

    <!-- please see http://confluence.jetbrains.com/display/IDEADEV/Build+Number+Ranges for description -->
    <idea-version since-build="131"/>

    <!-- please see http://confluence.jetbrains.com/display/IDEADEV/Plugin+Compatibility+with+IntelliJ+Platform+Products
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- ******************************** SCRIPT ********************************* -->
        <!-- Language -->
        <fileTypeFactory implementation="org.pcsoft.plugins.intellij.inno_setup.script.IssScriptFileTypeFactory"/>
        <fileTypeFactory implementation="org.pcsoft.plugins.intellij.inno_setup.script.IssLanguageFileTypeFactory"/>
        <!-- Parser & Syntax -->
        <lang.parserDefinition language="iss"
                               implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.IssParserDefinition"/>
        <lang.syntaxHighlighterFactory language="iss"
                                       implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.syntax.IssSyntaxHighlighterFactory"/>
        <colorSettingsPage
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.syntax.IssSyntaxColorSettingsPage"/>
        <!-- Structure -->
        <lang.psiStructureViewFactory language="iss"
                                      implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.structure.tree.IssStructureViewFactory"/>
        <lang.foldingBuilder language="iss"
                             implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.structure.folding.IssSectionFoldingBuilder"/>
        <!-- Others -->
        <lang.commenter language="iss"
                        implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.others.IssCommenter"/>
        <lang.braceMatcher language="iss"
                           implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.others.IssBraceMatcher"/>
        <lang.quoteHandler language="iss"
                           implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.others.IssQuoteHandler"/>
        <lang.formatter language="iss"
                        implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.structure.formatting.IssFormattingModelBuilder"/>
        <codeStyleSettingsProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.structure.formatting.IssCodeStyleSettingsProvider"/>
        <langCodeStyleSettingsProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.structure.formatting.IssLanguageCodeStyleSettingsProvider"/>

        <!-- Annotators -->
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.IssCommonAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.IssConstantAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.IssIdentifierAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionFileAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionDirectoryAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionComponentAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionTaskAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionTypeAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionIconAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionInstallRunAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionUninstallRunAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionINIAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionRegistryAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionLanguageAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionInstallDeleteAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.section.IssSectionUninstallDeleteAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.cd.IssCompilerDirectiveCommonAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.cd.IssCompilerDirectiveSymbolDefineSectionAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.cd.IssCompilerDirectiveSymbolUndefineSectionAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.cd.IssCompilerDirectivePreProcessorSectionAnnotator"/>
        <annotator language="iss"
                   implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.highlighting.annotator.cd.IssCompilerDirectiveIncludeSectionAnnotator"/>

        <!-- Contributors -->
        <!-- Completion -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.IssSectionHeaderCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.type.IssFontTypeCompletionContributorProperty"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.type.IssBooleanTypeCompletionContributorProperty"/>
        <!-- Compiler Directives -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.IssCompilerDirectiveCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.cd.IssVisibilityCompilerDirectiveValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.cd.IssPreProcessorTypeCompilerDirectiveValueCompletionContributor"/>
        <!-- Constants -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.constant.IssBuiltinConstantCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.constant.IssEnvironmentConstantCompletionContributor"/>
        <!-- Setup -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssSetupPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssTypeCompressionPropertyValueCompletionContributor"/>
        <!-- LanguageOption -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssLanguageOptionPropertyCompletionContributor"/>
        <!-- Task -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssTaskPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssTaskFlagPropertyValueCompletionContributor"/>
        <!-- Component -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssComponentPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssComponentFlagPropertyValueCompletionContributor"/>
        <!-- Type -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssTypePropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssTypeFlagPropertyValueCompletionContributor"/>
        <!-- File -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssFilePropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssFileFlagPropertyValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssIOCopyModePropertyValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssIOAttributePropertyValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssIOPermissionValueCompletionContributor"/>
        <!-- Directory -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssDirectoryPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssDirectoryFlagPropertyValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssIOAttributePropertyValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssIOPermissionValueCompletionContributor"/>
        <!-- Icon -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssIconPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssIconFlagPropertyValueCompletionContributor"/>
        <!-- Install Run -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssInstallRunPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssInstallRunFlagPropertyValueCompletionContributor"/>
        <!-- Uninstall Run -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssUninstallRunPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssUninstallRunFlagPropertyValueCompletionContributor"/>
        <!-- INI -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssINIPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssINIFlagPropertyValueCompletionContributor"/>
        <!-- Registry -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssRegistryPropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssRegistryFlagPropertyValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssRegistryRootPropertyValueCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssRegistryValueTypePropertyValueCompletionContributor"/>
        <!-- Language -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssLanguagePropertyCompletionContributor"/>
        <!-- InstallDelete -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssInstallDeletePropertyCompletionContributor"/>
        <!-- UninstallDelete -->
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.property.IssUninstallDeletePropertyCompletionContributor"/>
        <completion.contributor language="iss"
                                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.contributors.completion.value.IssDeleteTypePropertyValueCompletionContributor"/>

        <!-- Go To -->
        <gotoSymbolContributor
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.contributors.go_to.IssSectionTaskGoToContributor"/>
        <gotoSymbolContributor
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.contributors.go_to.IssSectionTypeGoToContributor"/>
        <gotoSymbolContributor
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.contributors.go_to.IssSectionComponentGoToContributor"/>

        <!-- Manipulators -->
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.property.IssPropertyComponentReferenceValueElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.property.IssPropertyComponentReferenceValueElementManipulator"/>
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.property.IssPropertyNameValueElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.property.IssPropertyNameValueElementManipulator"/>
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.property.IssPropertyTaskReferenceValueElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.property.IssPropertyTaskReferenceValueElementManipulator"/>
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.property.IssPropertyTypeReferenceValueElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.property.IssPropertyTypeReferenceValueElementManipulator"/>
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.property.IssPropertyLanguagesReferenceValueElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.property.IssPropertyLanguagesReferenceValueElementManipulator"/>
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.property.common.IssIdentifierReferenceElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.property.common.IssIdentifierReferenceElementManipulator"/>
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.constant.IssConstantNameElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.constant.IssConstantNameElementManipulator"/>
        <lang.elementManipulator
                forClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.elements.cd.parameter.IssCompilerDirectiveIdentifierParameterElement"
                implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.parser.psi.manipulators.cd.parameter.IssCompilerDirectiveIdentifierParameterElementManipulator"/>

        <lang.refactoringSupport language="iss"
                                 implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.others.IssRefactoringSupportProvider"/>
        <statementUpDownMover implementation="org.pcsoft.plugins.intellij.inno_setup.script.others.IssStatementUpDownMover"/>

        <!-- Documentation -->
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionComponentDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionTypeDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionTaskDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionFileDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionDirectoryDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionIconDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionInstallRunDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionUninstallRunDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionINIDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionRegistryDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionSetupDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionReferenceDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.section.IssSectionCommonDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.IssConstantDocumentationProvider"/>
        <documentationProvider
                implementation="org.pcsoft.plugins.intellij.inno_setup.script.doc.IssCompilerDirectiveDocumentationProvider"/>

        <!-- Line Marker -->
        <codeInsight.lineMarkerProvider language="iss"
                                        implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.references.marker.IssOverviewLineMarkerProvider"/>
        <codeInsight.lineMarkerProvider language="iss"
                                        implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.references.marker.IssReferenceLineMarkerProvider"/>
        <codeInsight.lineMarkerProvider language="iss"
                                        implementationClass="org.pcsoft.plugins.intellij.inno_setup.script.references.marker.IssLanguageLineMarkerProvider"/>

        <!-- ************************* RUN & COMPILE ****************************** -->
        <configurationType implementation="org.pcsoft.plugins.intellij.inno_setup.configuration.run.IssCompileRunConfigurationType"/>
        <runConfigurationProducer implementation="org.pcsoft.plugins.intellij.inno_setup.configuration.run.IssCompileRunConfigurationProducer"/>
        <applicationService serviceImplementation="org.pcsoft.plugins.intellij.inno_setup.configuration.app.IssCompilerSettings"/>
        <applicationConfigurable provider="org.pcsoft.plugins.intellij.inno_setup.configuration.app.IssCompilerConfiguration" parentId="project.propCompiler"/>

        <!-- ************************* OTHERS ****************************** -->
        <virtualFileSystem key="iss-file-system" implementationClass="org.pcsoft.plugins.intellij.inno_setup.vfs.IssVirtualFileSystem"/>
        <toolWindow factoryClass="org.pcsoft.plugins.intellij.inno_setup.ui.IssBaseToolWindowFactory"
                    conditionClass="org.pcsoft.plugins.intellij.inno_setup.ui.IssBaseToolWindowCondition"
                    canCloseContents="false" anchor="right" id="Inno Setup"/>
        <toolWindow factoryClass="org.pcsoft.plugins.intellij.inno_setup.ui.IssHelpToolWindowFactory"
                    conditionClass="org.pcsoft.plugins.intellij.inno_setup.ui.IssHelpToolWindowCondition"
                    canCloseContents="false" anchor="bottom" id="Inno Setup Help"/>
        <errorQuickFixProvider implementation="org.pcsoft.plugins.intellij.inno_setup.script.quickfix.IssQuickFixProvider"/>
    </extensions>

    <application-components>
        <!-- Add your application components here -->
    </application-components>

    <project-components>
        <!-- Add your project components here -->
    </project-components>

    <actions>
        <!-- Add your actions here -->
        <action class="org.pcsoft.plugins.intellij.inno_setup.action.IssOpenWithISIDEAction" id="ISS_OPEN_WITH_ISIDE">
            <add-to-group group-id="ProjectViewPopupMenu" anchor="last"/>
            <add-to-group group-id="StructureViewPopupMenu" anchor="last"/>
        </action>
        <action class="org.pcsoft.plugins.intellij.inno_setup.action.IssCompileWithISIDEAction" id="ISS_COMPILE_WITH_ISIDE">
            <add-to-group group-id="ProjectViewPopupMenu" anchor="last"/>
            <add-to-group group-id="StructureViewPopupMenu" anchor="last"/>
        </action>
        <action class="org.pcsoft.plugins.intellij.inno_setup.action.IssCreateScriptAction" id="ISS_CREATE_SCRIPT">
            <add-to-group group-id="NewGroup" anchor="last"/>
        </action>
        <action class="org.pcsoft.plugins.intellij.inno_setup.script.action.IssGenerateAppIdAction" id="ISS_GENERATE_APP_ID">
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>
        </action>
    </actions>

</idea-plugin>